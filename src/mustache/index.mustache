<!DOCTYPE html>
<html>
    <head>
        <title> {{title}} </title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <meta charset="utf-8">
        <link href="./styles.css?v{{version}}" rel="stylesheet">
    </head>
    <body>
        <div id="init-loader" class="valign-wrapper">
            <div>
                <div>
                    <div><em>{{loading}}</em></div>

                    <noscript>
                        {{javascript}}
                    </noscript>
                </div>
                <div class="progress">
                    <div class="indeterminate"></div>
                </div>
            </div>
         </div>
        <div id="nav">
            <header class="left">
                <span class="icon"><i class="fas fa-fw fa-map-marker-alt"></i></span><span data-mobile="{{header_short}}">{{header}}</span>
            </header>
            <ul class="right">
                <!-- <li data-tooltip="{{export}}" id="save" ><a class="waves-effect waves-light btn-flat" href="#!"><i class="fas fa-file-download"></i></a></li> -->
                <li data-tooltip="{{help}}" id="help"><a class="waves-effect waves-light btn-flat" href="#!"><i class="fas fa-question-circle"></i></a></li>
                <li data-tooltip="{{about}}" id="infos"><a class="waves-effect waves-light btn-flat" href="#!"><i class="fas fa-info"></i></a></li>
                <li data-tooltip="{{fullscreen}}" id="fullscreen"><a class="waves-effect waves-light btn-flat" href="#!"><i class="fas fa-expand"></i></a></li>
            </ul>
            <div id="loading"></div>
        </div>
        <div id="sidepanel">
            <div id="sidepanel-tools">
                <a id="sidepanel-closer" href="#!" class="waves-effect waves-light">
                    <i class="fas fa-fw fa-angle-left"></i>
                </a>
                <a id="list-toggle" href="#!" class="waves-effect waves-light">
                    <i class="fas fa-fw fa-list"></i>
                    <span id="list-count">1</span>
                </a>
            </div>
        </div>
        <div id="list">
            <header>

            </header>
            <div class="list-content">
            </div>
        </div>
        <div id="modal" class="modal modal-fixed-header">
            <div class="modal-header">
                <h3 class="modal-title"></h3>
                <a href="#!" class="closer right waves-effect waves-red btn-flat"><i class="fas fa-times"></i></a>
            </div>
            <div class="modal-content"></div>
        </div>
        <div id="map"></div>
        <div id="map-tools">
            <div id="zoom">
                <a id="zoom-in" class="waves-effect waves-light"><i class="fa fa-fw fa-plus"></i></a>
                <a id="zoom-out" class="waves-effect waves-light"><i class="fa fa-fw fa-minus"></i></a>
            </div>
        </div>
        <script src="./scripts.js?v{{version}}"></script>
        <link href="./fonts/roboto/roboto.css" rel="stylesheet">
        <link href="./fonts/fontawesome/fontawesome.min.css" rel="stylesheet">
    </body>
</html>
