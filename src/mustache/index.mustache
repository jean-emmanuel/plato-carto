<!DOCTYPE html>
<html>
    <head>
        <title> {{title}} </title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <meta charset="utf-8">
        <link href="./styles.css" rel="stylesheet">
    </head>
    <body>
        <div id="nav">
            <header class="left">
                <span class="icon"><i class="fas fa-fw fa-map-marker-alt"></i></span>{{header}}
            </header>
            <ul class="right">
              <li><a class="waves-effect waves-light btn-small">Contact</a></li>
            </ul>
            <div id="loading"></div>
        </div>
        <div id="sidepanel">
            <div id="sidepanel-tools">
                <div id="sidepanel-closer" class="waves-effect waves-light">
                    <i class="fas fa-fw fa-angle-left"></i>
                </div>
                <div id="list-toggle" class="waves-effect waves-light">
                    <i class="fas fa-fw fa-list"></i>
                    <span id="list-count">1</span>
                </div>
            </div>
        </div>
        <div id="list">
            <header>

            </header>
            <div class="list-content">
            </div>
        </div>
        <div id="modal" class="modal modal-fixed-header">
            <div class="modal-header">
                <h3 class="modal-title"></h3>
                <a href="#!" class="modal-close right waves-effect waves-red btn-flat"><i class="fas fa-times"></i></a>
            </div>
            <div class="modal-content"></div>
        </div>
        <div id="map"></div>
        <div id="map-tools">
            <div id="zoom">
                <a id="zoom-in" class="waves-effect waves-light"><i class="fa fa-fw fa-plus"></i></a>
                <a id="zoom-out" class="waves-effect waves-light"><i class="fa fa-fw fa-minus"></i></a>
            </div>
        </div>
        <script src="./scripts.js"></script>
        <link href="./fonts/roboto/roboto.css" rel="stylesheet">
        <link href="./fonts/fontawesome/fontawesome.min.css" rel="stylesheet">
    </body>
</html>
